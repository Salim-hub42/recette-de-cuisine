{% extends 'base.html.twig' %}

{% block title %}Nous contacter{% endblock %}

{% block body %}
<h1>Contactez-nous</h1>
    {{ form_start(form, {'attr': {'data-turbo': 'false'}}) }}
    <div class="row">
        <div class="col-sm">
            {{ form_row(form.name) }}
        </div>
        <div class="col-sm">
              {{ form_row(form.email) }}
        </div>
          <div class="col-sm">
              {{ form_row(form.service) }}
        </div>
    </div>
    {{ form_end(form) }}
{% endblock %}
